-var staticBaseUrl = '../public/ajax/libs/'

-var staticCDNBaseUrl = '//resource.fenqimall.com/ajax/libs/'





mixin ie (sCondition, eCondition)

	|<!--[!{sCondition}>

	block

	|<!!{eCondition}]-->





-var loaderUrl = '//resource.fenqimall.com/ajax/libs/js/requirejs/2.1.11/require.min.js'

mixin jsloader (cdn, boot)

	if !cdn

		script(data-main= staticBaseUrl + boot, src= loaderUrl)

	else

		script(data-main= staticCDNBaseUrl + boot, src= loaderUrl)





mixin importCSS (cdn, specificUrl)

	if !cdn

		link(rel= 'stylesheet', href= staticBaseUrl + specificUrl)

	else

		link(rel= 'stylesheet', href= staticCDNBaseUrl + specificUrl)





mixin importJS (cdn, specificUrl)

	if !cdn

		script(src= staticBaseUrl + specificUrl)

	else

		script(src= staticCDNBaseUrl + specificUrl)





